<?xml version="1.0" encoding="UTF-8"?>
<Planets>
    <planet>
        <id>1</id>
        <name>Mercury</name>
        <type>closest planet to sun</type>
        <temp>800 degrees Fahrenheit </temp>
        <moon>no moons</moon>
    </planet>
    <planet>
        <id>2</id>
        <name>Venus</name>
        <type>second planet from the sun</type>
        <temp>900 degrees Fahrenheit </temp>
        <moon>no moons</moon>
    </planet>
    <planet>
        <id>3</id>
        <name>Earth</name>
        <type>third planet from the sun</type>
        <temp>59 F</temp>
        <moon>one moon</moon>
    </planet>
    <planet>
        <id>4</id>
        <name>Mars</name>
        <type>fourth planet from the sun</type>
        <temp>-171 to 32 F</temp>
        <moon>two moons</moon>
    </planet>
    <planet>
        <id>5</id>
        <name>Jupiter</name>
        <type>largest planet in our solar system</type>
        <temp>-234 F</temp>
        <moon>63 moons</moon>
    </planet>
    <planet>
        <id>6</id>
        <name>Saturn</name>
        <type>The Ringed One</type>
        <temp>-300F to -188F</temp>
        <moon>146 moons</moon>
    </planet>
    <planet>
        <id>7</id>
        <name>Uranus</name>
        <type>four times wider than Earth</type>
        <temp>-320 F</temp>
        <moon>27 moons</moon>
    </planet>
    <planet>
        <id>8</id>
        <name>Neptune</name>
        <type>farthest one from the sun</type>
        <temp>-353 F</temp>
        <moon>14 moons</moon>
    </planet>
    <planet>
        <id>9</id>
        <name>Pluto</name>
        <type>one of the coldest large bodies</type>
        <temp>-369 F</temp>
        <moon>5 moons</moon>    
    </planet>
    <planet>
        <id>10</id>
        <name>J1407b</name>
        <type>Saturn on steroids”</type>
        <temp>434 light years from earth</temp>
        <moon>640 times that of Saturn’s rings</moon>
    </planet>
    <planet>
        <id>11</id>
        <name>Gliese 581c </name>
        <type>A Potentially Habitable Exoplanet</type>
        <temp>20 light years from earth</temp>
        <moon>Gliese 581c is tidally locked</moon>
    </planet>
    <planet>
        <id>12</id>
        <name>Gj 1214b</name>
        <type>The Waterworld</type>
        <temp>GJ 1214b has no land</temp>
        <moon>oceans stretching over its surface</moon> 
    </planet>
    <planet>
        <id>13</id>
        <name>Gliese 436b</name>
        <type>A Planet Defying The Laws Of Physics</type>
        <temp>30 light years from Earth </temp>
        <moon>822°F</moon> 
    </planet>
    <planet>
        <id>14</id>
        <name>55 Cancri E </name>
        <type>A Diamond Planet</type>
        <temp>40 light years from Earth </temp>
        <moon>4417 degrees F</moon> 
    </planet>
    <planet>
        <id>15</id>
        <name>Hat-P-7b</name>
        <type>Where It Rains Rubies And Sapphires</type>
        <temp>1000 light years from Earth </temp>
        <moon>aluminum oxide is found in the atmosphere</moon>  
    </planet>
    <planet>
        <id>16</id>
        <name>Psr J1719-1483 B</name>
        <type>Orbits Around A Pulsar</type>
        <temp>an extremely compact and dense neutron star</temp>
        <moon>has an orbital period of 2 hours</moon>  
    </planet>
    <planet>
        <id>17</id>
        <name>Hd 189733b</name>
        <type>Where It Rains Glass</type>
        <temp>62 light years away from Earth</temp>
        <moon>Temperatures can also reach over 900°C</moon>  
    </planet>
    <planet>
        <id>18</id>
        <name>Wasp-12b</name>
        <type>A Planet That’s Eating Up Light</type>
        <temp>This weird planet, which is twice the size of Jupiter</temp>
        <moon>temperature of its atmosphere is a soaring 4,600°C</moon>  
    </planet>
    <planet>
        <id>19</id>
        <name>Gj-504b</name>
        <type>The Pink Planet</type>
        <temp>the resident of the Virgo constellation</temp>
        <moon>it orbits its star from nearly nine times the distance that Jupiter orbits the sun</moon>
    </planet>
    <planet>
        <id>20</id>
        <name>Kepler-10c</name>
        <type>A Mega-Earth</type>
        <temp>weighs 17 times as much as Earth and is more than twice as large</temp>
        <moon>560 light-years from Earth</moon>
    </planet>
</Planets>
